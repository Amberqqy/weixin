.box-header {
  padding: 20rpx 48rpx;
}
.big-size {
  font-size: 44rpx;
}
.little-size {
  font-size: 22rpx;
  background-color:rgba(177,153,255,0.2); 
  color: #B199FF; 
  border-radius: 4rpx;
  margin-left:14rpx;
  padding: 2rpx 6rpx;
}
.now-size {
  font-size: 22rpx;
  background-color: rgba(0,174,102,0.2);
  color: #00AE66;
  border-radius: 4rpx; 
  margin-left: 12rpx;
  padding: 2rpx 6rpx; 
}
.else-name {
  font-size: 26rpx;
  margin: 10rpx 0;
  color: #6B7072;
}
.box-img {
  width: 100%;
  padding: 0 50rpx;
  margin-bottom: 50rpx;
  box-sizing: border-box;
}
.new-image {
  height: 340rpx;
  position: relative;
}
.img-left:first-child{
  display: inline-block; 
  vertical-align: top; 
  width: 60%;
  height: 100%;
}
.img-left:nth-child(2){
  display: inline-block; 
  width: 40%;
  vertical-align: top; 
  height: 50%;
}
 .img-left:nth-child(3) {
  display: inline-block; 
  width: 40%;
  vertical-align: top; 
  height: 50%;
  position: relative;
  top: -169rpx;
  left: 390rpx; 
}
.hipe-view {
  position: absolute;
  width: 40%;
  vertical-align: top; 
  height: 50%;
  background: rgba(0, 0, 0, 0.75);
  bottom: 0;
  right: 0;
  z-index: 55;
  text-align: center;
  color: #fff;
}
.box-list {
  display: flex;
  width: 100%;
  padding: 14rpx 0;
}
.list-text {
  flex: 1;
  margin-left: 5%;
  border-right: 1rpx solid #e5e5e5;
  display: inline-block;
  text-align: left;
}
.list-text:first-child { 
  margin-left:0%;
} 
.list-text:last-child { 
   border-right:none; 
} 
.tal-text {
  display: block;
  font-size: 26rpx;
  color: #9399A5;
  margin-bottom: 3rpx;
}
.num-text {
  font-size: 32rpx;
  color: #FA5741;
}
.about-box {
  padding: 14rpx 0;
}
.box-about {
  display: block;
  margin-bottom: 12rpx;
  position: relative;
}
.box-about::after {
   position: absolute;
   right:0;
   top: 22rpx;
   display: inline-block;
   content: "";
   width: 12rpx;
   height: 12rpx;
   border: solid #9C9FA1;
   border-width:4rpx 4rpx 0 0 ;
   transform: rotate(45deg);  
}
.black-text {
  font-size: 30rpx;
  color: #333;
}
.time-text {
  font-size: 30rpx;
  color: aquamarine;
  margin-left: 20rpx;
}
.more-house {
  color:#3072F6;
  font-size: 32rpx;
  border-radius: 10rpx;
}
.clearfix::after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.more-house::after{
  border: none;
}
.box-tab {
  border-top: 16rpx solid #e5e5e5;
  padding: 0 40rpx;
}
.tab-text {
  display: inline-block; 
  margin-right:30rpx;
  padding-top: 0 
}
.tab-list {
  width: 100%;
  /* height: 128rpx; */
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}

.list-image {
  flex: 1; 
  height:160rpx; 
  background: pink;
  vertical-align: top;
}
.house-list {
  flex: 2;
  height: 100%;
  font-size: 0;
  margin-left: 40rpx;
  vertical-align: top;
  /* background: yellowgreen; */
}
.list-round {
   width: 390rpx;
   height: 200rpx;
   display: inline-block;
   margin-right: 40rpx; 
   background: pink;
}
.house-list>view {
  margin-bottom: 8rpx;
}
.lala {
  font-size: 0;
}
.lala .round-tal {
  display: inline-block; 
   width: 300rpx;  
  overflow: hidden; 
  color: #394043;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: top;
  font-size: 32rpx;
  letter-spacing: 0;
  line-height: 32rpx;
}
.house-big {
  font-size: 32rpx; 
  font-weight: 550;
  vertical-align: top; 
  color: #9399A5;
  letter-spacing: 0;
  /* margin-right: 5rpx; */
}
.house-aear {
  font-size: 24rpx;
  color: #6B7072;
  margin-right: 6rpx;
}
.house-have {
  background: #fff;
  color: #9C9FA1;
  font-size: 22rpx;
  border: 2rpx solid #e5e5e5;
  padding:0 6rpx;
  margin-right: 6rpx;
}
.lala {
  margin: 0 0;
}
.house-num {
  font-size: 32rpx;
  color: #FE615A;
  letter-spacing: 0;
}
.round {
  margin: 40rpx 0 0 0;
  border-top: 14rpx solid #e5e5e5;
  padding: 30rpx 50rpx 10rpx 50rpx;
}
.round-text {
  font-size: 38rpx;
  font-weight: 560;
  color: #000;
}
.round-more {
  font-size: 26rpx;
  color: #9C9FA1;
  float: right; 
  position: relative;
  right: 30rpx;
  top: 14rpx;
  vertical-align: bottom;
}
.box-round {
  white-space: nowrap; 
  display:flex;
  width:100%; 
  padding: 30rpx 0;
}
.btn-buttom {
  width: 100%;
  margin: 20rpx 0 10rpx 0;
  text-align: center;
}
.btn-lg {
  font-size: 32rpx;
  width: 45%;
  display: inline-block;
  padding: 8rpx 0; 
  border-radius: 200rpx;
}
.btn-left {
   margin-right: 5%;
}
.btn-left::after {
  border: none;
}
.btn-right::after {
  border: none;
}
.buy-info {
  position: relative;
  padding: 30rpx 50rpx;
  border-bottom: 2rpx solid #e5e5e5;
}
.buy-info:last-child {
  border-bottom: 0;
}
.info-log {
  position: absolute;
  left:6rpx;
  top: 36rpx;
  background-color: #FE615A;
  color: #fff;
  font-size: 20rpx;
  padding: 4rpx 5rpx;
}
.info-tatal {
  display: block;
  margin-bottom: 10rpx;
  font-size: 32rpx;
}
.info-people {
  display: inline-block;
  font-size: 26rpx;
  color: #9399A5;
  position: relative;
}
.info-hr {
  display: inline-block;
  font-size: 26rpx;
  color: #9399A5;
  position: relative;
}
.info-hr:last-child::after{
  background: #fff;
}
.info-hr::after {
  display: inline-block;
  content: "";
  clear: both;
  width: 2rpx;
  height: 20rpx;
  right: 6rpx;
  background: #9399A5;
  top: -4rpx;
  margin: 0 10rpx;
}
/* .info-people:last-child {
  border-right: 0;
} */
.rice-mark {
  display: inline-block;
  padding: 16rpx 36rpx;
  font-size: 24rpx;
  background:  #F3F4F5;
  border-radius: 30rpx;
  margin-right: 18rpx;
  margin-bottom: 20rpx;
}
.hide {
  display: none
}
.show {
  display: block;
}
.round .mask-all {
   margin: 20rpx 0; 
  font-size: 32rpx;
  line-height: 46rpx;
  color: #101D37;
  /* display: -webkit-box;
  -webkit-box-orient: vertical; 
  -webkit-line-clamp: 3;
  overflow: hidden;  */
}
.round .else-all {
   margin: 20rpx 0; 
  font-size: 32rpx;
  line-height: 46rpx;
  color: #101D37;
  width: 100%;
  display: inline-block;
}